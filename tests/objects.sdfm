	var price : Int;

	init(){

	}

	function plusOne() -> Int{
		age = age + 1;
		return age;
	}

	function hola(l : Int){
		age = age + l;
		price = price + 2;
	}
}

function factorial( n : Int) -> Int{
	if n == 1{
		return 1;
	}
	return n * factorial(n-1);
}



